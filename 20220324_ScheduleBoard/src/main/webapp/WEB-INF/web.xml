<?xml version="1.0" encoding="UTF-8"?>
	<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
  <display-name>20220324_ScheduleBoard</display-name>
  <welcome-file-list>
	<welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>CalController</servlet-name>
    <servlet-class>com.min.edu.ctrl.CalController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CalController</servlet-name>
    <url-pattern>/CalController.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>CalListAjax</servlet-name>
    <servlet-class>com.min.edu.ctrl.CalListAjax</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CalListAjax</servlet-name>
    <url-pattern>/CalListAjax.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>DelController</servlet-name>
    <servlet-class>com.min.edu.ctrl.DelController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DelController</servlet-name>
    <url-pattern>/DelController.do</url-pattern>
  </servlet-mapping>

</web-app>